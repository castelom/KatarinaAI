{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Gemini": {
    "Model": "gemini-2.5-flash-lite"
  },
  "GitHub": {
    "BaseApiUrl": "https://api.github.com"
  },
  "Project": {
    "Name": "YOUR_PROJECT_NAME",

    // Project Repositories
    // If your project uses a single repository for both frontend and backend (monorepo),
    // just configure one entry below.
    // If your frontend and backend are in separate repositories,
    // add one entry for each repository.
    "Repositories": [
      {
        "Name": "backend",
        "RepositoryUrl": "YOUR_PROJECT_BACKEND_URL",
        "Branch": "YOUR_PROJECT_BRANCH",
        "Type": "backend"
      },
      {
        "Name": "frontend",
        "RepositoryUrl": "YOUR_PROJECT_FRONTEND_URL",
        "Branch": "YOUR_PROJECT_BRANCH",
        "Type": "frontend"
      }
    ]
  }
}
